<santorini>
    <!--SampleGod XML
    <sampleGod>
        <movement>
            <standardMove active="true" priority="2">MOVE</standardMove>
            <additionalMoveNotInitialPosition active="false" priority="">MOVE</additionalMoveNotInitialPosition>
            <opponentSpaceMoveFlipPosition active="false" priority="">MOVE_OPPONENT_SLOT_FLIP</opponentSpaceMoveFlipPosition>
            <opponentSpaceMoveBackwards active="false" priority="">MOVE_OPPONENT_SLOT_PUSH</opponentSpaceMoveBackwards>
            <disableOpponentMoveUp active="false" priority=""></disableOpponentMoveUp>
            <modifiableParameter>
                <canMoveUp>true</canMoveUp>
            </modifiableParameter>
        </movement>

        <contruction>
            <standardBuild active="true" priority="">BUILD</standardBuild>
            <additionalBuildNotSamePlace active="false" priority="">BUILD_NOT_SAME_PLACE</additionalBuildNotSamePlace>
            <additionalBuildSamePlaceNotDome active="false" priority="">BUILD_SAME_PLACE_NOT_DOME</additionalBuildSamePlaceNotDome>
            <buildDomeAnyLevel active="false" priority="">BUILD_DOME_ANY_LEVEL</buildDomeAnyLevel>
            <buildBeforeMove active="false" priority="">BUILD</buildBeforeMove>
        </contruction>

        <winCondition>
            <standardWin>true</standardWin>
            <moveDownWin>False</moveDownWin>
        </winCondition>
    </sampleGod>
    -->

    <apollo>
        <moveSequence>
            <opponentSpaceMoveFlipPosition priority="1">MOVE_OPPONENT_SLOT_FLIP</opponentSpaceMoveFlipPosition>
            <standardBuild priority="2">BUILD</standardBuild>
        </moveSequence>
    </apollo>

    <artemis>
        <moveSequence>
            <standardMove priority="1">MOVE</standardMove>
            <additionalMoveNotInitialPosition priority="2">MOVE_NOT_INITIAL_POSITION</additionalMoveNotInitialPosition>
            <standardBuild priority="3">BUILD</standardBuild>
        </moveSequence>
    </artemis>

    <athena>
        <moveSequence>
            <disableOpponentMoveUpMove priority="1">MOVE_DISABLE_OPPONENT_UP</disableOpponentMoveUpMove>
            <standardBuild priority="2">BUILD</standardBuild>
        </moveSequence>
    </athena>

    <atlas>
        <moveSequence>
            <standardMove priority="1">MOVE</standardMove>
            <standardBuild priority="2">BUILD</standardBuild>
            <buildDomeAnyLevel priority="3">BUILD_DOME_ANY_LEVEL</buildDomeAnyLevel>
        </moveSequence>
    </atlas>

    <demeter>
        <moveSequence>
            <standardMove priority="1">MOVE</standardMove>
            <standardBuild priority="2">BUILD</standardBuild>
            <additionalBuildNotSamePlace priority="3">BUILD_NOT_SAME_PLACE</additionalBuildNotSamePlace>
        </moveSequence>
    </demeter>

    <hephaestus>
        <moveSequence>
            <standardMove priority="1">MOVE</standardMove>
            <standardBuild priority="2">BUILD</standardBuild>
            <additionalBuildSamePlaceNotDome priority="3">BUILD_SAME_PLACE_NOT_DOME</additionalBuildSamePlaceNotDome>
        </moveSequence>
    </hephaestus>

    <minotaur>
        <moveSequence>
            <opponentSpaceMoveBackwards priority="1">MOVE_OPPONENT_SLOT_PUSH</opponentSpaceMoveBackwards>
            <standardBuild priority="2">BUILD</standardBuild>
        </moveSequence>
    </minotaur>

    <pan>
        <moveSequence>
            <standardMove priority="1">MOVE</standardMove>
            <standardBuild priority="2">BUILD</standardBuild>
        </moveSequence>

        <winCondition>
            <moveDownWin>true</moveDownWin>
        </winCondition>
    </pan>

    <prometheus>
        <moveSequence>
            <buildBeforeMove priority="1">BUILD</buildBeforeMove>
            <standardMove priority="2">MOVE</standardMove>
            <standardBuild priority="3">BUILD</standardBuild>
        </moveSequence>
    </prometheus>
</santorini>
